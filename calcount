import java.util.*;

public class CalCount {

    private double calorieCount;
    private ArrayList<Double> calories;

    public CalCount(ArrayList<Double> cals){
        calories = cals;
        totalCalories();
    }

    private double addCal(){
        double totalCal = 0;
        for (int i = 0; i < calories.size(); i++){
            totalCal = calories.get(i);
        }
        return totalCal;
    }

    public double totalCalories(){
        System.out.println(addCal());

    }

}